<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_skill_india"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/s1"

    tools:context="com.example.w10.loginregister2.SkillIndia">

    <android.support.v7.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"

        android:minHeight="?attr/actionBarSize"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"

        android:id="@+id/toolbar"
        app:theme="@style/ThemeOverlay.AppCompat.Dark"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

    <Button
        android:text="ABOUT PROJECT"
        android:layout_width="300dp"
        android:layout_height="125dp"
        android:layout_marginTop="35dp"
        android:id="@+id/button"
        android:layout_below="@+id/toolbar"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:background="@drawable/corner"
        />


    <Button
        android:text="OBJECTIVES"
        android:layout_width="300dp"
        android:layout_height="125dp"
        android:layout_marginTop="35dp"
        android:id="@+id/button1"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:background="@drawable/corner" />


    <Button
        android:text="DEVELOPEMENT and PERFORMANCE"
        android:layout_width="300dp"
        android:layout_height="125dp"
        android:layout_marginTop="35dp"
        android:id="@+id/button2"
        android:layout_below="@+id/button1"
        android:layout_centerHorizontal="true"
        android:textSize="25dp"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        android:background="@drawable/corner" />

</RelativeLayout>
